{
    "name": "mrgabricavi/laravel-eloquent-repositories",
    "description": "Provides a scaffolding for repositories and models in Eloquent for Laravel",
    "keywords": ["framework", "laravel", "packages", "eloquent", "repository"],
    "license": "GPL-3.0-or-later",
    "require": {
        "laravel/framework": "^9"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^6.0|^7.0",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "MrGabriCavi\\LaravelEloquentRepositories\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MrGabriCavi\\LaravelEloquentRepositories\\Tests\\": "tests/"
        }
    },
    "scripts": {
    },
    "extra": {
        "laravel": {
            "providers": [
                "MrGabriCavi\\LaravelEloquentRepositories\\Providers\\LaravelEloquentRepositoriesServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
